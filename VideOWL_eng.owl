                <owl:onClass rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">Series of games</rdfs:comment>
        <rdfs:comment xml:lang="ita">le serie che contengono i giochi</rdfs:comment>
    </owl:Class>
    


