<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#"
     xml:base="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://www.semanticweb.org/porta/ontologies/2022/5/VideOWL#terms:">
    <owl:Ontology rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/VideOWL">
        <terms:created>Marianna Nicolosi-Asmundo</terms:created>
        <terms:created>Simone De Martino</terms:created>
        <terms:created>Stefano Angelo Rizzo</terms:created>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/VideOWL#terms:created -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/VideOWL#terms:created"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#NPCOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#NPCOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#characterOf"/>
        <rdfs:comment xml:lang="en">Non-playing character of a game</rdfs:comment>
        <rdfs:comment xml:lang="ita">personaggio non giocabile nel videogame</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#POVOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#POVOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#featureOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasPOV"/>
        <rdfs:comment xml:lang="en">Perspective employed to visualize the game</rdfs:comment>
        <rdfs:comment xml:lang="ita">prospettiva impiegata per visualizzare il gioco</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#antagonistOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#antagonistOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#NPCOf"/>
        <rdfs:comment xml:lang="en">Antagonists and enemies</rdfs:comment>
        <rdfs:comment xml:lang="ita">antagonisti e nemici</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#characterOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#characterOf">
        <rdfs:comment xml:lang="en">property used to define the fictitious characters (therefore, not the real living developers) of a game</rdfs:comment>
        <rdfs:comment xml:lang="ita">proprietà che serve a definire i personaggi fittizi (quindi, non gli sviluppatori realmente viventi) di un gioco</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#colleagueOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#colleagueOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Single"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Single"/>
        <rdfs:comment xml:lang="en">Symmetric property that manages the working relationships between various developers of the same title</rdfs:comment>
        <rdfs:comment xml:lang="ita">proprietà simmetrica che gestisce i rapporti di collaborazione lavorativa tra vari sviluppatori di un titolo</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#compatibleWith -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#compatibleWith">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasFeature"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#platformOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Platform"/>
        <rdfs:comment xml:lang="en">Refer to its inverse.</rdfs:comment>
        <rdfs:comment xml:lang="ita">vedi la sua inversa</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#contextOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#contextOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#featureOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasContext"/>
        <rdfs:comment xml:lang="en">Specific context applied to a gaming experience</rdfs:comment>
        <rdfs:comment xml:lang="ita">contesto specifico applicato a un&apos;esperienza videoludica</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#deuteragonistOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#deuteragonistOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#playableOf"/>
        <rdfs:comment xml:lang="en">co-protagonist</rdfs:comment>
        <rdfs:comment xml:lang="ita">co-protagonista</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#developedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#developedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasFeature"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#developerOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Developer"/>
        <rdfs:comment xml:lang="en">Refer to its inverse.</rdfs:comment>
        <rdfs:comment xml:lang="ita">vedi la sua inversa</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#developerOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#developerOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#featureOf"/>
        <rdfs:comment xml:lang="en">Real person that contributed to a videogame development</rdfs:comment>
        <rdfs:comment xml:lang="ita">persona reale che ha sviluppato un titolo specifico</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#employedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#employedBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hires"/>
        <rdfs:comment xml:lang="en">Subordination relationship of a developer to an employer. It has no reference domain as it may be both a development company or another lead developer, but also other special cases of relationship.</rdfs:comment>
        <rdfs:comment xml:lang="ita">rapporto di subordinazione di uno sviluppatore a un datore di lavoro, non ha dominio di riferimento in quanto può essere sia una casa di sviluppo o un altro sviluppatore-capo, ma anche altri casi particolari</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#employeeOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#employeeOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Single"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#SoftwareHouse"/>
        <rdfs:comment xml:lang="en">Describes a working relationship directly between individual developer and software house</rdfs:comment>
        <rdfs:comment xml:lang="ita">descrive un rapporto di lavoro direttamente tra singolo developer e software house</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#featureOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#featureOf">
        <rdfs:comment xml:lang="ita">macroproprietà che racchiude al suo interno tutto ciò che può essere impiegato da un individual-descrizione per mettersi in relazione con un individual-videogioco, con individual-attore o con un individual-piattaforma</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#gameplayOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#gameplayOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#featureOf"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasGameplay"/>
        <rdfs:comment xml:lang="en">Specific gameplay in a gaming experience</rdfs:comment>
        <rdfs:comment xml:lang="ita">tipo di gameplay è inserito in un gioco</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasContext -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasContext">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasFeature"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Context"/>
        <rdfs:comment xml:lang="en">Refer to its inverse.</rdfs:comment>
        <rdfs:comment xml:lang="ita">vedi la sua inversa</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasFeature -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasFeature">
        <rdfs:comment xml:lang="en">The conceptual inverse of &quot;featureof&quot;, but having as subject the single game rather than the individual of the characteristic: for this reason, there will be always &quot;Game&quot; as their domain. Please refer to the dataproperty mentioned above for clarification on individual properties.</rdfs:comment>
        <rdfs:comment xml:lang="ita">concettualmente l&apos;inversa di caratteristicadi, ma con soggetto il gioco singolo piuttosto che l&apos;individual della caratteristica: per questo hanno sempre Game come dominio. Si rimanda alla dataproperty sopra menzionata per avere chiarimenti sulle singole proprietà.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasGameplay -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasGameplay">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasFeature"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Gameplay"/>
        <rdfs:comment xml:lang="en">Refer to its inverse.</rdfs:comment>
        <rdfs:comment xml:lang="ita">vedi la sua inversa</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasMechanic -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasMechanic">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasFeature"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#mechanicOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Mechanic"/>
        <rdfs:comment xml:lang="en">Refer to its inverse.</rdfs:comment>
        <rdfs:comment xml:lang="ita">vedi la sua inversa</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasNarrative -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasNarrative">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasFeature"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#narrationOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Narrative"/>
        <rdfs:comment xml:lang="en">Refer to its inverse.</rdfs:comment>
        <rdfs:comment xml:lang="ita">vedi la sua inversa</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasPOV -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasPOV">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasFeature"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#POV"/>
        <rdfs:comment xml:lang="en">Refer to its inverse.</rdfs:comment>
        <rdfs:comment xml:lang="ita">vedi la sua inversa</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hires -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hires">
        <rdfs:comment xml:lang="en">Describes the subordination relationship from an employer to a developer</rdfs:comment>
        <rdfs:comment xml:lang="ita">Descrive il rapporto di subordinazione da un datore di lavoro a uno sviluppatore</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#includes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#includes">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#partOf"/>
        <rdfs:comment xml:lang="ita">Serve a identificare un rapporto di inclusione generico</rdfs:comment>
        <rdfs:comment xml:lang="en">Used to identify a generic inclusion relationship</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#mechanicOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#mechanicOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#featureOf"/>
        <rdfs:comment xml:lang="en">Specific game mechanic used in a title</rdfs:comment>
        <rdfs:comment xml:lang="ita">meccanica programmata in un titolo</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#narrationOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#narrationOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#featureOf"/>
        <rdfs:comment xml:lang="en">Specific narrative line adopted in a video game</rdfs:comment>
        <rdfs:comment xml:lang="ita">linea narrativa adottata in un videogame</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#partOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#partOf">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:comment xml:lang="en">It is used to identify a generic inclusion relationship</rdfs:comment>
        <rdfs:comment xml:lang="ita">impiegato per identificare un rapporto di inclusione generico</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#platformOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#platformOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#featureOf"/>
        <rdfs:comment xml:lang="en">Device that can successfully start the game</rdfs:comment>
        <rdfs:comment xml:lang="ita">dispositivo che può avviare correttamente il game</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#playableOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#playableOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#characterOf"/>
        <rdfs:comment xml:lang="en">Character impersonable in a game</rdfs:comment>
        <rdfs:comment xml:lang="ita">personaggio impersonabile in un titolo</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#prequelOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#prequelOf">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#sequelOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
        <rdfs:comment xml:lang="en">Transitive property that establishes whether a game is a prequel to another game</rdfs:comment>
        <rdfs:comment xml:lang="ita">proprietà transitiva che stabilisce se un gioco è prequel di un altro gioco</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#protagonistOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#protagonistOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#playableOf"/>
        <rdfs:comment xml:lang="en">Main character</rdfs:comment>
        <rdfs:comment xml:lang="ita">protagonista principale</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#secondaryOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#secondaryOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#NPCOf"/>
        <rdfs:comment xml:lang="en">Secondary characters</rdfs:comment>
        <rdfs:comment>personaggi secondari</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#sequelOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#sequelOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment xml:lang="en">Transitive property that establishes whether a game is a sequel to another game</rdfs:comment>
        <rdfs:comment xml:lang="ita">proprietà transitiva che stabilisce se un gioco è sequel di un altro gioco</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasReleaseDate -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#hasReleaseDate">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/porta/ontologies/2022/5/untitled-ontology-7#Game"/>
